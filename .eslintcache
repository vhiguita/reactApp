tity;\n      console.log(price);\n      total = total + price;\n      j = j + 1;\n    }\n    setTotalPrice(total);\n    setNumProd(n);\n    console.log(\"cantidad de productos:\"+n);\n  }\n  const isInCart = (id) =>{\n    //console.log(product);\n    return product.findIndex(prod =>prod.item.id === id);\n  }\n  const clear = () =>{\n    setProduct([]);\n    setTotalPrice(0);\n    setIsCartEmpty(true);\n    setNumProd(0);\n    //console.log(product);\n  }\n  return (\n\n    <cartContext.Provider value={{\n      product,\n      addItem,\n      removeItem,\n      clear,\n      isCartEmpty,\n      totalPrice,\n     }}>\n     {children}\n    </cartContext.Provider>\n\n  );\n}\n\nexport default CartContext;\n",{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":26,"column":29,"nodeType":"81","messageId":"82","endLine":26,"endColumn":31},{"ruleId":"83","severity":1,"message":"84","line":16,"column":6,"nodeType":"85","endLine":16,"endColumn":30},{"ruleId":"86","severity":1,"message":"87","line":11,"column":20,"nodeType":"88","messageId":"89","endLine":11,"endColumn":27},{"ruleId":"83","severity":1,"message":"84","line":33,"column":8,"nodeType":"85","endLine":33,"endColumn":31},{"ruleId":"86","severity":1,"message":"90","line":1,"column":16,"nodeType":"88","messageId":"89","endLine":1,"endColumn":24},{"ruleId":"86","severity":1,"message":"91","line":1,"column":38,"nodeType":"88","messageId":"89","endLine":1,"endColumn":47},{"ruleId":"86","severity":1,"message":"92","line":9,"column":9,"nodeType":"88","messageId":"89","endLine":9,"endColumn":23},{"ruleId":"86","severity":1,"message":"93","line":10,"column":20,"nodeType":"88","messageId":"89","endLine":10,"endColumn":27},{"ruleId":"83","severity":1,"message":"84","line":73,"column":30,"nodeType":"85","endLine":73,"endColumn":76},{"ruleId":"86","severity":1,"message":"94","line":15,"column":10,"nodeType":"88","messageId":"89","endLine":15,"endColumn":17},"no-nati